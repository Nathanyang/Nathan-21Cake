{% endblock %}
<!-- /container -->
<!-- Start global Js-->
{% block globalJs %}
    {% stylesheets
        '@NathanCakeBundle/Resources/public/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js'
        '@NathanCakeBundle/Resources/public/js/jquery.min.js'
        '@NathanCakeBundle/Resources/public/js/vendor/bootstrap.min.js'
        '@NathanCakeBundle/Resources/public/js/main.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endstylesheets %}
{% endblock %}
<!-- End global Js-->
<!-- Start My Js-->
{% block MyJs %}
{% endblock %}
<!-- end My Js-->
</body>
</html>
